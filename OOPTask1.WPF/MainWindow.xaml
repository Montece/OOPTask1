        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Loaded="Window_Loaded" Unloaded="Window_Unloaded"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
        mc:Ignorable="d"
        Title="OOPTask1" Height="125" Width="328">
    <StackPanel>
        <Button Name="ChooseFile_Button" Content="Выбрать файл..." Margin="10 10 10 10" Click="ChooseFile_Button_Click"/>
        <Button Name="Execute_Button" Content="Обработать файл" Margin="10 10 10 10" Click="Execute_Button_Click"/>
    </StackPanel>
</Window>
